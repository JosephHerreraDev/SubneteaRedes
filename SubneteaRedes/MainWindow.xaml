<Window
    x:Class="SubneteaRedes.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:System="clr-namespace:System;assembly=mscorlib"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:SubneteaRedes"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="Subnetea Redes"
    Width="349.477"
    Height="432.1"
    ResizeMode="NoResize"
    mc:Ignorable="d">
    <Window.Background>
        <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
            <GradientStop Offset="1" Color="Black" />
            <GradientStop Offset="0.494" Color="#FF4EB7C7" />
            <GradientStop Offset="0.974" Color="White" />
        </LinearGradientBrush>
    </Window.Background>
    <Grid Margin="0,0,2,19">
        <GroupBox
            x:Name="groupBox"
            Width="317"
            Height="150"
            Margin="10,10,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            BorderBrush="{x:Null}"
            Header="Ingresa la red a subnetear">
            <Grid
                Width="303"
                Margin="4,10,-2,13"
                HorizontalAlignment="Left">
                <Label
                    x:Name="label1"
                    Width="20"
                    Height="23"
                    Margin="227,20,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Content="/" />
                <TextBox
                    x:Name="txbSufijo"
                    Width="50"
                    Height="23"
                    Margin="247,20,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    TabIndex="5"
                    TextChanged="TxbSufijo_TextChanged"
                    TextWrapping="Wrap" />
                <Button
                    x:Name="btnSubnetear"
                    Width="86"
                    Height="23"
                    Margin="118,73,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Click="BtnSubnetear_Click"
                    Content="Subnetear"
                    TabIndex="6" />
                <DockPanel
                    x:Name="Red1"
                    Width="188"
                    Height="30"
                    Margin="39,19,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    LastChildFill="False">
                    <TextBox
                        x:Name="txbPrimerOcteto"
                        Width="35"
                        Height="23"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top"
                        TabIndex="1"
                        TextChanged="TxbPrimerOcteto_TextChanged"
                        TextWrapping="Wrap" />
                    <Label
                        x:Name="label2_Copy3"
                        Width="14"
                        Height="23"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top"
                        Content="." />
                    <TextBox
                        x:Name="txbSegundoOcteto"
                        Width="35"
                        Height="23"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top"
                        TabIndex="2"
                        TextChanged="TxbSegundoOcteto_TextChanged"
                        TextWrapping="Wrap" />
                    <Label
                        x:Name="label2_Copy4"
                        Width="14"
                        Height="23"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top"
                        Content="." />
                    <TextBox
                        x:Name="txbTercerOcteto"
                        Width="35"
                        Height="23"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top"
                        TabIndex="3"
                        TextChanged="TxbTercerOcteto_TextChanged"
                        TextWrapping="Wrap" />
                    <Label
                        x:Name="label2_Copy5"
                        Width="14"
                        Height="23"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top"
                        Content="." />
                    <TextBox
                        x:Name="txbCuartoOcteto"
                        Width="35"
                        Height="23"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top"
                        TabIndex="4"
                        TextChanged="TxbCuartoOcteto_TextChanged"
                        TextWrapping="Wrap" />
                </DockPanel>
            </Grid>
        </GroupBox>
        <GroupBox
            x:Name="groupBox1"
            Width="317"
            Height="170"
            Margin="10,165,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Header="Mascara &amp; Wildcard">
            <Grid
                Width="295"
                Margin="0,10,-2,13"
                HorizontalAlignment="Right">
                <Label
                    x:Name="label"
                    Width="54"
                    Height="26"
                    Margin="21,1,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Content="Mascara" />
                <Label
                    x:Name="label_Copy"
                    Width="67"
                    Height="26"
                    Margin="21,59,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Content="Wildcard" />
                <DockPanel
                    x:Name="Red1_Copy"
                    Width="188"
                    Height="30"
                    Margin="21,85,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    LastChildFill="False">
                    <TextBox
                        x:Name="txbWildcard1"
                        Width="35"
                        Height="23"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top"
                        Background="#00000000"
                        BorderBrush="#00000000"
                        IsReadOnly="True"
                        TextWrapping="Wrap" />
                    <Label
                        x:Name="label2_Copy6"
                        Width="14"
                        Height="23"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top"
                        Content="." />
                    <TextBox
                        x:Name="txbWildcard2"
                        Width="35"
                        Height="23"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top"
                        Background="#00000000"
                        BorderBrush="#00000000"
                        IsReadOnly="True"
                        TextWrapping="Wrap" />
                    <Label
                        x:Name="label2_Copy7"
                        Width="14"
                        Height="23"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top"
                        Content="." />
                    <TextBox
                        x:Name="txbWildcard3"
                        Width="35"
                        Height="23"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top"
                        Background="#00000000"
                        BorderBrush="#00000000"
                        IsReadOnly="True"
                        TextWrapping="Wrap" />
                    <Label
                        x:Name="label2_Copy8"
                        Width="14"
                        Height="23"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top"
                        Content="." />
                    <TextBox
                        x:Name="txbWildcard4"
                        Width="35"
                        Height="23"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top"
                        Background="#00000000"
                        BorderBrush="#00000000"
                        IsReadOnly="True"
                        TextWrapping="Wrap" />
                </DockPanel>
                <DockPanel
                    x:Name="Red2"
                    Width="188"
                    Height="36"
                    Margin="21,27,0,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    LastChildFill="False">
                    <TextBox
                        x:Name="txbMascara1"
                        Width="35"
                        Height="23"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top"
                        Background="#00000000"
                        BorderBrush="#00000000"
                        IsReadOnly="True"
                        TextWrapping="Wrap" />
                    <Label
                        x:Name="label2_Copy"
                        Width="14"
                        Height="23"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top"
                        Content="." />
                    <TextBox
                        x:Name="txbMascara2"
                        Width="35"
                        Height="23"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top"
                        Background="#00000000"
                        BorderBrush="#00000000"
                        IsReadOnly="True"
                        TextWrapping="Wrap" />
                    <Label
                        x:Name="label2_Copy1"
                        Width="14"
                        Height="23"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top"
                        Content="." />
                    <TextBox
                        x:Name="txbMascara3"
                        Width="35"
                        Height="23"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top"
                        Background="#00000000"
                        BorderBrush="#00000000"
                        IsReadOnly="True"
                        TextWrapping="Wrap" />
                    <Label
                        x:Name="label2_Copy2"
                        Width="14"
                        Height="23"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top"
                        Content="." />
                    <TextBox
                        x:Name="txbMascara4"
                        Width="35"
                        Height="23"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top"
                        Background="#00000000"
                        BorderBrush="#00000000"
                        IsReadOnly="True"
                        TextWrapping="Wrap" />
                </DockPanel>
            </Grid>
        </GroupBox>
        <Button
            x:Name="btnVerRedes"
            Width="104"
            Height="20"
            Margin="120,352,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Click="BtnVerRedes_Click"
            Content="Ver Redes"
            TabIndex="7" />

    </Grid>
</Window>
